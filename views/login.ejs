    <title>Sign up</title>
  </head>
  <body>
   <div class="container">
    <div class="col-md-8 col-sm-10 col-lg-6 m-5" id="form-container">
      <form action="/users/login"METHOD="POST" class="form-group form" >
        <span id="userHead"><h2>Login</h2></span>
        <span id="userHead" class="text-align-center"> <p> Not registered? <a href="/users/register" class="text-decoration-none">Sign up</a> </p> </span>
        <% include ./partials/messages %>
          <input type="Email" id="email" name="email" class="form-control my-3 p-2.5" placeholder="Email Address" name="email" required autofocus>
          <input type="password" id="password" name="password" class="form-control mb-2 " placeholder="Password" name="password" required>
          <a href="" class="text-decoration-none">Forgot Password?</a>
          <button  type="submit" class="btn color-white text-align-center font-weight-bold form-control my-2">Login</button>
          
      </form>
  </div>
       
   </div>



    <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js" integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo" crossorigin="anonymous"></script>
